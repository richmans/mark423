<div>
<h2><%=t(:login)%> <%=t(:to_mark423)%></h2>
<%= form_tag '/login' do %>
  <%= label_tag t :email%> <%= text_field_tag :email %><br/>
  <%= label_tag t :password%> <%= password_field_tag :password %><br/>
  <%= submit_tag t(:login) %>
<% end %>
<%= link_to t(:forgot_password), forgot_path %>
</div>